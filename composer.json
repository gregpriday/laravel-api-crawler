{
    "name": "gregpriday/laravel-api-crawler",
    "description": "Crawls a site's API using internal Kernel.",
    "type": "package",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.7"
    },
    "authors": [
        {
            "name": "Greg Priday",
            "email": "greg@siteorigin.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GregPriday\\APICrawler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GregPriday\\APICrawler\\Tests\\": "tests/"
        }
    },
    "extras" : {
        "laravel" : {
            "providers": [
                "GregPriday\\APICrawler\\CrawlerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
